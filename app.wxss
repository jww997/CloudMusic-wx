@keyframes rotate {
  0% {
    transform: rotate(0);
  }

  100% {
    transform: rotate(360deg);
  }
}

text {
  font-family: PingFang SC;
  font-size: 25rpx;
}

.clear {
  clear: both;
}

.wrap {
  width: 690rpx;
  margin: 0 auto;
}

.flex-wrap {
  display: flex;
  flex-wrap: wrap;
  width: 100%;
}

.padding-top {
  padding-top: 30rpx;
}

.padding-bottom {
  padding-bottom: 30rpx;
}

