Page {
  height: 100%;
}

.square {
  height: 100%;
  display: flex;
  flex-direction: column;
}

.square-nav {
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #eee;
  color: #333;
}

.square-nav-list {
  flex-grow: 1;
  display: flex;

  height: 100%;
  overflow: scroll;
  position: relative;
}

.square-nav-list-line {
  width: 100%;
  height: 2px;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 0 25rpx;
  box-sizing: border-box;
}

.square-nav-list-line > view {
  width: 40rpx;
  height: 100%;
  border-radius: 50rpx;
  background-color: #f00;
  position: absolute;
}

.square-nav-list-item {
  flex-shrink: 0;
  padding: 0 20rpx;
  font-size: 26rpx;
  height: 100%;
}

.square-nav-list-item-active {
  color: #f00;
  font-weight: bold;
}

.icon-classify {
  margin: 0 20rpx;
  font-size: 30rpx;
}

swiper {
  flex-grow: 1;
}





















.wrap {
  width: 690rpx;
  height: 100%;
  margin: 0 auto;
  overflow: scroll;
}

.songlist {
  display: flex;
  flex-wrap: wrap;
}

.songlist-item {
  margin: 0 15rpx;
}
